№1
x = 1  # Замените значение x на то, которое вы хотите проверить

if -30 <= x < -10:
    print("Точка x принадлежит интервалу [-30;-10)")
else:
    print("Точка x не принадлежит интервалу [-30;-10)")


№2
x = -5  # Замените значение x на то, которое вы хотите проверить

if (-10 < x <= -5) or (-2 <= x <= 2):
    print(f"Точка x = {x} принадлежит интервалу (-10;-5]U[-2;2]")
else:
    print(f"Точка x = {x} не принадлежит интервалу (-10;-5]U[-2;2]")
